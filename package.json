{
  "name": "tycho",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run build-css && react-scripts test --env=jsdom",
    "coverage": "rimraf coverage && npm run build-css && react-scripts test --env=jsdom --coverage",
    "lint": "eslint src/*",
    "ephemeris": "tycho-scripts -m ephemeris",
    "orbitals": "tycho-scripts -m orbitals"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "deep-assign": "2.0.0",
    "js-cookie": "2.2.0",
    "keymirror": "0.1.1",
    "moment": "2.19.2",
    "react": "15.6.1",
    "react-animation-frame": "0.3.4",
    "react-datetime": "2.10.1",
    "react-dom": "15.6.1",
    "react-markdown": "2.5.0",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.13",
    "react-slider": "0.9.0",
    "react-three-renderer": "3.2.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "three": "0.87.0",
    "three-dom-label": "0.1.6",
    "three-orbit-controls": "82.1.0",
    "tween.js": "16.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "codacy-coverage": "^2.0.3",
    "enzyme": "2.9.1",
    "enzyme-to-json": "2.0.0",
    "eslint": "^4.1.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest-cli": "21.0.2",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "4.1.2",
    "react-test-renderer": "15.6.1",
    "rimraf": "2.6.2",
    "tycho-scripts": "^1.0.3"
  }
}
