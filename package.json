{
  "name": "tycho",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "deep-assign": "^2.0.0",
    "jest-cli": "^21.0.2",
    "js-cookie": "^2.1.4",
    "keymirror": "^0.1.1",
    "moment": "^2.18.1",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-animation-frame": "0.3.4",
    "react-datetime": "^2.10.1",
    "react-dom": "^15.6.1",
    "react-markdown": "^2.5.0",
    "react-scripts": "^1.0.13",
    "react-slider": "^0.9.0",
    "react-three-renderer": "^3.2.1",
    "redux-thunk": "^2.2.0",
    "three": "0.87.1",
    "three-orbit-controls": "^82.1.0",
    "tween.js": "^16.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "npm run build-css && react-scripts test --env=jsdom",
    "test-scripts": "mocha --reporter dot --require ./scripts/__tests__/index.js ./scripts/__tests__/*.test.js",
    "coverage": "rimraf coverage && npm run build-css && react-scripts test --env=jsdom --coverage"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "codecov": "^2.3.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "2.0.0",
    "mocha": "^3.5.3",
    "node-sass-chokidar": "^0.0.3",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "rimraf": "^2.6.1",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0"
  }
}
